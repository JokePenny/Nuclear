                                        <TextBlock.Background>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_map_interface.png"/>
                                        </TextBlock.Background>
                                    </TextBlock>
                                    <TextBlock Height="19" Width="40" Opacity="0" Margin="-6 2 0 0" MouseLeftButtonDown="Opacity_ClickDown" MouseLeftButtonUp="Collapsed_WindowUI_Click" Tag="CharacteristicOpen">
                                        <TextBlock.Background>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_cha_interface.png"/>
                                        </TextBlock.Background>
                                    </TextBlock>
                                    <TextBlock Height="19" Width="40" Opacity="0" Margin="-6 2 0 0" MouseLeftButtonDown="Opacity_ClickDown" MouseLeftButtonUp="Collapsed_WindowUI_Click">
                                        <TextBlock.Background>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_pip_interface.png"/>
                                        </TextBlock.Background>
                                    </TextBlock>
                                    <TextBlock Height="19" Width="40" Opacity="0" Margin="-6 2 0 0" MouseLeftButtonDown="Opacity_ClickDown" MouseLeftButtonUp="Collapsed_WindowUI_Click" Tag="CraftOpen">
