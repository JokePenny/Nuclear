                                <Grid.RowDefinitions>
                                    <RowDefinition Height="25"></RowDefinition>
                                    <RowDefinition Height="75"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <WrapPanel Grid.Row="0" HorizontalAlignment="Center" Margin="7">
                                    <Ellipse Grid.Row="0" Width="8" Height="8" Opacity="1" Margin="4 0 0 0" >
                                        <Ellipse.Fill>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_lamp_interface.png"/>
                                        </Ellipse.Fill>
                                    </Ellipse>
                                    <Ellipse Grid.Row="0" Width="8" Height="8" Opacity="1" Margin="1 0 0 0">
                                        <Ellipse.Fill>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_lamp_interface.png"/>
                                        </Ellipse.Fill>
                                    </Ellipse>
                                    <Ellipse Grid.Row="0" Width="8" Height="8" Opacity="1" Margin="1 0 0 0">
                                        <Ellipse.Fill>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_lamp_interface.png"/>
                                        </Ellipse.Fill>
                                    </Ellipse>
                                    <Ellipse Grid.Row="0" Width="8" Height="8" Opacity="1" Margin="1 0 0 0">
                                        <Ellipse.Fill>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_lamp_interface.png"/>
                                        </Ellipse.Fill>
                                    </Ellipse>
                                    <Ellipse Grid.Row="0" Width="8" Height="8" Opacity="1" Margin="2 0 0 0">
                                        <Ellipse.Fill>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_lamp_interface.png"/>
                                        </Ellipse.Fill>
                                    </Ellipse>
                                    <Ellipse Grid.Row="0" Width="8" Height="8" Opacity="1" Margin="1 0 0 0">
                                        <Ellipse.Fill>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_lamp_interface.png"/>
                                        </Ellipse.Fill>
                                    </Ellipse>
                                    <Ellipse Grid.Row="0" Width="8" Height="8" Opacity="1" Margin="1 0 0 0">
                                        <Ellipse.Fill>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_lamp_interface.png"/>
                                        </Ellipse.Fill>
                                    </Ellipse>
                                    <Ellipse Grid.Row="0" Width="8" Height="8" Opacity="1" Margin="1 0 0 0">
                                        <Ellipse.Fill>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_lamp_interface.png"/>
                                        </Ellipse.Fill>
                                    </Ellipse>
                                    <Ellipse Grid.Row="0" Width="8" Height="8" Opacity="1" Margin="1 0 0 0">
                                        <Ellipse.Fill>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_lamp_interface.png"/>
                                        </Ellipse.Fill>
                                    </Ellipse>
                                    <Ellipse Grid.Row="0" Width="8" Height="8" Opacity="0" Margin="1 0 0 0">
                                        <Ellipse.Fill>
                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_lamp_interface.png"/>
