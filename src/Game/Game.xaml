                                            <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_fix_interface.png"/>
                                        </TextBlock.Background>
                                    </TextBlock>
                                </StackPanel>
                                <TextBlock Grid.Row="2" Grid.Column="1"/>
                            </Grid>
                        </StackPanel>
                    </Grid>
                    <!--
                        <Button Background="gray" Content="Конец хода"/>
                        <Button Background="white" Content="Назад" Click="OpenGameField_Click"/>
                        <Button Background="white" Content="Навыки" Click="OpenSkillTree_Click"/>
                        <Button Background="white" Content="ИНвентарь" Click="OpenBackpack_Click"/>
                        -->
                </StackPanel>
                <StackPanel Grid.Row="0" Grid.Column="0" Grid.RowSpan="2">
                    <!--<Button Width="70" Background="silver" Click="Exit_Game"/>-->
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="130"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <ScrollViewer Grid.Row="0"  Grid.ColumnSpan="2" Name="Chat"  VerticalScrollBarVisibility="Hidden" Padding="0 30 0 30">
                            <TextBlock Margin="25 0 10 0" Name="ChatTextBlock" TextWrapping="Wrap" Foreground="Lime" FontFamily="/Nuclear;component/data/fonts/#Fallout Display" FontSize="8"/>
                        </ScrollViewer>
                        <TextBox Height="20" Margin="30 95 20 0" Name="MessageTextBox" Grid.Row="0" Grid.Column="0" Foreground="Lime" KeyDown="SendButton_Key" Background="{x:Null}" />
                    </Grid>
                </StackPanel>
            </Grid>
        </StackPanel>
        <!-- интерфейс -->

        <!-- умения -->
