                                    <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_red_interface.png"/>
                                </Ellipse.Fill>
                            </Ellipse>
                            <TextBlock Height="22" Width="33" Grid.Row="1" Opacity="0" Margin="1 14 0 0" MouseLeftButtonDown="Opacity_ClickDown" MouseLeftButtonUp="Collapsed_WindowUI_Click" Tag="InventoryOpen">
