                                    <ImageBrush ImageSource="/Nuclear;component/data/image/mainui/interfaceClick/but_menu_interface.png"/>
                                </TextBlock.Background>
                            </TextBlock>
                        </StackPanel>
                        <StackPanel Grid.Row="0" Grid.Column="1" Orientation="Vertical" Margin="0 0 0 0">
